t\t} else {\n\t\t\tevent.returnValue = false;\n\t\t}\n\t}\n\t// 1뎁스 카테고리 선택탭\n\tvar i = 0;\n\tvar cate = $('#indexSch .link_category'); // 1뎁스 메뉴\n\tvar select = cate.eq(i).parent();\n\tvar con = $('#indexSch .select_search'); // 1뎁스 내용\n\t// 기본 탭 노출\n\tselect.addClass('on');\n\tcon.eq(i).show();\n\tcate.click(function (){\n\t\tif(layerOn.hasClass('layer_on')){\n\t\t\tlayerClose();\n\t\t}\n\t\tj = 0; // 2뎁스 인덱스 초기화\n\n\t\tselect.removeClass('on');\n\t\tcon.eq(i).hide();\n\n\t\ti = cate.index(this);\n\t\tselect = $(this).parent();\n\n\t\tselect.addClass('on');\n\t\tcon.eq(i).show();\n\n\t\tpreD();\n\t});\n\t// 2뎁스 선택탭\n\tvar j = 0\n\tvar subCate = $('#indexSch .link_sub'); // 2뎁스 메뉴\n\tvar subSel = subCate.eq(j).parent().parent();\n\tvar layerOn = $('#indexSch .box_tab');\n\tsubCate.click(function (){\n\t\tif($(this).parent().parent().hasClass('off')){\n\t\t\t$(this).click(false); // 미선택시 클릭 비활성\n\t\t}else if($(this).parent().parent().hasClass('on')){\n\t\t\tlayerClose(); // 토글기능\n\t\t}else{\n\t\t\tsubSel.removeClass('on');\n\n\t\t\tj = subCate.index(this);\n\t\t\tsubSel = $(this).parent().parent();\n\n\t\t\tif(subSel.hasClass('off')){\n\t\t\t\tsubSel.removeClass('off');\n\t\t\t}\n\t\t\tlayerOn = $(this).parents('.box_tab');\n\t\t\tlayerOn.addClass('layer_on');\n\t\t\tsubSel.addClass('on');\n\t\t}\n\t\tpreD();\n\t});\n\t// 레이어 메뉴 선택시 다음탭 레이어 오픈\n\tvar itemSel = $('#indexSch .link_item'); // 레이어 안 아이템 링크\n\titemSel.click(function (){\n\t\tsubSel.removeClass('on');\n\t\t// 다음탭 메뉴에 해당 아이템 텍스트 넣기\n\t\tvar itemText = $(this).text();\n\t\tvar subText = subCate.eq(j).children('.txt_sub')\n\t\tsubText.text(itemText);\n\t\t// on 부여\n\t\tvar test = $(this).parents('.area_search').find('li').removeClass('on');\n\t\t$(this).parent().addClass('on');\n\t\t// 각 카테고리탭에서 마지막탭 처리\n\t\tif(j !== 2 && j !== 5){\n\t\t\tj += 1;\n\t\t\tsubSel = subCate.eq(j).parent().parent();\n\n\t\t\tif(subSel.hasClass('off')){\n\t\t\t\tsubSel.removeClass('off');\n\t\t\t}\n\n\t\t\tsubSel.addClass('on');\n\t\t}else{\n\t\t\tlayerOn.removeClass('layer_on');\n\t\t}\n\t\tpreD();\n\t});\n\t// 2뎁스 레이어 닫기 이벤트\n\tvar btnClose = $('#indexSch .btn_close');\n\tvar layerClose = function(){\n\t\tlayerOn.removeClass('layer_on');\n\t\tsubSel.removeClass('on');\n\t}\n\tbtnClose.click(function (){\n\t\tlayerClose();\n\t})\n}\nsearchTab();\n// 숫자 카운팅 에니메이션\nUInumbercount.init(1000); // 2017-02-09 [종] 스크립트 수정\n</script> <!-- 퍼블확인용 스크립트 -->\n\t\t\t</div>\n\t\t\t<div class=\"main_index\">\n\t\t\t\t<div class=\"section_mocar\">\n\t\t\t\t\t<h3 class=\"tit_mocar\"><a href=\"#\" class=\"ir_index\">엔카의 자동차 리뷰, 이 차 어때?</a></h3> <!-- 2018/04/13 [종] 텍스트수정 -->\n\t\t\t\t\t<div class=\"showRollWrap slide_mocar\">\n\t\t\t\t\t  <div class=\"showRoll\">\n\t\t\t\t\t\t<ul>\n\t\t\t\t\t\t\t<c:forEach var = \"i\" begin = \"0\" end = \"2\">\n\t\t\t\t\t\t\t<li class=\"scene\">\n\t\t\t\t\t\t\t\t<c:choose>\n\t\t\t\t\t\t\t\t<c:when test=\"${param.contents == 'load'}\">\n\t\t\t\t\t\t\t\t<img src=\"/images/index/loading.gif\" width=\"50\" height=\"63\" class=\"img_loading\" alt=\"loading\"> <!-- 2017/02/01 [종] 사이즈 변경 -->\n\t\t\t\t\t\t\t\t</c:when>\n\t\t\t\t\t\t\t\t<c:otherwise>\n\t\t\t\t\t\t\t\t<c:forEach var = \"j\" begin = \"0\" end = \"1\">\n\t\t\t\t\t\t\t\t<a href=\"#\" class=\"link_mocar\">\n\t\t\t\t\t\t\t\t\t<img src=\"http://img.encar.com/carsdata/cars/cm_cardb/file/012_039_2015_l.jpg\" width=\"124\" height=\"76\" class=\"img_thumb\" alt=\"\">\n\t\t\t\t\t\t\t\t\t<div class=\"cont_mocar\">\n\t\t\t\t\t\t\t\t\t\t<strong class=\"tit_subject\">현대 LF 쏘나타 하이브리드</strong>\n\t\t\t\t\t\t\t\t\t\t<dl class=\"list_review\">\n\t\t\t\t\t\t\t\t\t\t\t<dt class=\"ir_index tit_advantage\">장점</dt>\n\t\t\t\t\t\t\t\t\t\t\t<dd>본의 아닌 ’떼빙’의 가능성이 높음 가능성이 높음</dd>\n\t\t\t\t\t\t\t\t\t\t\t<dt class=\"ir_index tit_disadvantage\">단점</dt>\n\t\t\t\t\t\t\t\t\t\t\t<dd>본의 아닌 ’떼빙’의 가능성이 높음</dd>\n\t\t\t\t\t\t\t\t\t\t\t<dt class=\"ir_index tit_score\">종합</dt>\n\t\t\t\t\t\t\t\t\t\t\t<dd>\n\t\t\t\t\t\t\t\t\t\t\t\t<span class=\"ir_index ico_score\">